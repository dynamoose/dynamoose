{
  "name": "dynamoose",
  "description": "Dynamoose is a modeling tool for Amazon's DynamoDB (inspired by Mongoose)",
  "homepage": "https://dynamoosejs.com",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "jest": "^29.7.0",
    "lerna": "^6.6.2",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "prepare": "lerna bootstrap && npm run build",
    "build": "lerna run build",
    "build:sourcemap": "lerna run build:sourcemap",
    "build:sourcemap:watch": "lerna run build:sourcemap:watch",
    "build:clean": "lerna run build:clean",
    "build:watch": "lerna run build:watch",
    "test": "lerna run test",
    "test:types": "lerna run test:types",
    "lint": "eslint . --ext .ts,.js --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "site:install": "cd docs && npm install",
    "site:crowdin:sync": "cd docs && npm run crowdin:sync",
    "site:start": "cd docs && npm start",
    "site:build": "cd docs && npm run build",
    "site:swizzle": "cd docs && npm run swizzle",
    "performance": "cd packages/dynamoose && npm run performance",
    "performance:build": "cd packages/dynamoose && npm run performance:build",
    "performance:run": "cd packages/dynamoose && npm run performance:run",
    "performance:compare": "cd packages/dynamoose && npm run performance:compare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dynamoose/dynamoose.git"
  },
  "author": {
    "name": "Charlie Fish",
    "email": "fishcharlie.code@gmail.com",
    "url": "https://charlie.fish"
  },
  "contributors": [
    {
      "name": "Brandon Goode"
    }
  ],
  "bugs": {
    "url": "https://github.com/dynamoose/dynamoose/issues"
  },
  "dependencies": {
    "js-object-utilities": "^2.2.1"
  },
  "license": "Unlicense",
  "keywords": [
    "dynamodb",
    "dynamo",
    "mongoose",
    "aws",
    "amazon",
    "document",
    "model",
    "schema",
    "database",
    "data",
    "datastore",
    "query",
    "scan",
    "nosql",
    "db",
    "nosql",
    "store",
    "document store",
    "table",
    "json",
    "object",
    "storage"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "funding": [
    {
      "type": "github-sponsors",
      "url": "https://github.com/sponsors/fishcharlie"
    },
    {
      "type": "opencollective",
      "url": "https://opencollective.com/dynamoose"
    }
  ]
}

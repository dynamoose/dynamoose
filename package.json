{
  "name": "dynamoose",
  "version": "2.1.0-alpha.6",
  "description": "Dynamoose is a modeling tool for Amazon's DynamoDB (inspired by Mongoose)",
  "main": "dist/index.js",
  "homepage": "https://github.com/dynamoosejs/dynamoose#readme",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist",
    "build:watch": "tsc -w",
    "prepare": "npm run build:clean; npm run build",
    "test": "nyc --all mocha && nyc report --reporter=html",
    "test-nocoverage": "mocha",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dynamoosejs/dynamoose.git"
  },
  "author": {
    "name": "Charlie Fish",
    "email": "fishcharlie.code@gmail.com",
    "url": "https://charlie.fish"
  },
  "contributors": [
    {
      "name": "Brandon Goode"
    }
  ],
  "bugs": {
    "url": "https://github.com/dynamoosejs/dynamoose/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.653.0"
  },
  "license": "Unlicense",
  "keywords": [
    "dynamodb",
    "dynamo",
    "mongoose",
    "aws",
    "amazon",
    "document",
    "model",
    "schema",
    "database",
    "data",
    "datastore",
    "query",
    "scan",
    "nosql",
    "db",
    "nosql",
    "store",
    "document store",
    "table",

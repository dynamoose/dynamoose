{
  "name": "dynamoose",
  "version": "2.0.0-beta.3",
  "description": "Dynamoose is a modeling tool for Amazon's DynamoDB (inspired by Mongoose)",
  "main": "lib/index.js",
  "homepage": "https://github.com/dynamoosejs/dynamoose#readme",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0"
  },
  "scripts": {
    "test": "nyc --all mocha --recursive && npm run report-coverage",
    "test-ci": "nyc --all mocha --recursive && npm run report-coverage-text",
    "test-nocoverage": "mocha --recursive",
    "lint": "eslint .",
    "report-coverage": "nyc report --reporter=html",
    "report-coverage-text": "mkdir -p ./coverage; nyc report --reporter=text-lcov > ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dynamoosejs/dynamoose.git"
  },
  "author": "Charlie Fish & Brandon Goode",
  "bugs": {
    "url": "https://github.com/dynamoosejs/dynamoose/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.625.0",
    "chai-as-promised": "^7.1.1"
  },
  "license": "Unlicense",
  "keywords": [
    "dynamodb",
    "dynamo",
    "mongoose",
    "aws",
    "amazon",
    "document",
    "model",
    "schema",
    "database",
    "data",
    "datastore",
    "query",
    "scan",
    "nosql",
    "db",
    "nosql",
    "store",
    "document store",
    "table",
    "json"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib"
  ]
}
